@ARTICLE{DynamicFog2023,
  author={Hammoud, Ahmad and Kantardjian, Maria and Najjar, Amir and Mourad, Azzam and Otrok, Hadi and Dziong, Zbigniew and Guizani, Nadra},
  journal={IEEE Transactions on Network and Service Management}, 
  title={Dynamic Fog Federation Scheme for Internet of Vehicles}, 
  year={2023},
  volume={20},
  number={2},
  pages={1913-1923},
  keywords={Quality of service;Vehicle dynamics;Cloud computing;Internet of Vehicles;Games;Heuristic algorithms;Task analysis;Internet of Things;Internet of Vehicles;fog computing;game theory},
  doi={10.1109/TNSM.2022.3217972},
  abstract = {联邦雾计算是横向扩展雾资源以提高物联网（IoT）应用服务质量（QoS）的一种解决方案。然而，某些物联网关键组件（如车联网（IoV）组件）的动态特性可能会阻碍服务质量的改善，反而导致其恶化。具体来说，由于雾节点上的服务分配不优化和网络流量负载不平衡，可能会导致延迟。目前的雾联盟架构在组建雾联盟时忽略了用户的移动性。在这项工作中，我们根据服务需求，利用博弈论提出了一种自适应的高效雾联盟组建方案。组建联盟和在雾成员之间卸载请求的问题表述为一个整数程序，然后被模拟为一个赫顿博弈。我们采用 "合并与拆分"（Merge & Split）作为组建技术，即在 QoS 方面不满意的联盟与其他能提高服务性能的联盟合并。我们的自适应雾联盟形成机制旨在应对物联网范式中的环境变化。实验评估表明，与文献相比，我们的框架可以获得更好的 QoS，并缩短组建联盟的时间。}}

